{
  "name": "ReactNativeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "cross-env webpack-dev-server --progress --color --open",
    "build:android": "cd ./android && ./gradlew assembleRelease",
    "build:ios": "cd ./ios && xcodebuild archive -workspace ReactNativeProject.xcworkspace -scheme ReactNativeProject -destination generic/platform=iOS build -configuration release -archivePath ./build/archive/ReactNativeProject.xcarchive && xcodebuild -exportArchive -archivePath ./build/archive/ReactNativeProject.xcarchive -exportPath ./build/archive/ -exportOptionsPlist ./exportOptionsAdHoc.plist -allowProvisioningUpdates",
    "build:web": "webpack --mode production",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.10",
    "react-native-web": "^0.19.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.2.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.19.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
